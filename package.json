{
  "name": "ember-emeis",
  "version": "8.0.1",
  "description": "The frontend for the emeis user management service",
  "keywords": [
    "ember-addon",
    "ember-engine",
    "emeis",
    "caluma"
  ],
  "repository": "github:projectcaluma/ember-emeis",
  "license": "LGPL-3.0-or-later",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:pkg": "npmPkgJsonLint .",
    "start": "ember serve",
    "start-proxy": "ember server --proxy http://localhost:8000",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.hbs": "ember-template-lint --fix",
    "package.json": "npmPkgJsonLint .",
    "*.{scss,graphql,json,md,yml}": "prettier --write"
  },
  "dependencies": {
    "@ember/legacy-built-in-components": "^0.4.2",
    "@ember/render-modifiers": "^2.0.5",
    "@embroider/macros": "^1.12.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "ember-auto-import": "^2.5.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.1.1",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^2.1.2",
    "ember-data": "^3.28.3",
    "ember-inflector": "^4.0.2",
    "ember-intl": "^5.7.2",
    "ember-modifier": "^3.2.7",
    "ember-power-select": "^6.0.1",
    "ember-resources": "^6.1.1",
    "ember-set-helper": "^2.0.1",
    "ember-test-selectors": "^6.0.0",
    "ember-truth-helpers": "^3.1.1",
    "ember-uikit": "^8.0.0",
    "file-saver": "^2.0.5",
    "tracked-toolbox": "^2.0.0"
  },
  "devDependencies": {
    "@adfinis/eslint-config": "2.0.0",
    "@adfinis/semantic-release-config": "4.0.0",
    "@babel/core": "7.22.9",
    "@babel/eslint-parser": "7.22.6",
    "@babel/plugin-proposal-decorators": "7.22.6",
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.9.3",
    "@embroider/compat": "3.1.4",
    "@embroider/core": "3.1.2",
    "@embroider/test-setup": "3.0.1",
    "@embroider/webpack": "3.1.2",
    "@faker-js/faker": "6.2.0",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "7.6.0",
    "ember-cli": "4.12.1",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-mirage": "3.0.0-alpha.3",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-uglify": "3.0.0",
    "ember-engines": "0.9.0",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-qunit": "6.2.0",
    "ember-resolver": "8.0.3",
    "ember-source": "4.12.3",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "5.11.0",
    "ember-template-lint-plugin-prettier": "4.2.0",
    "ember-try": "3.0.0-beta.1",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-ember": "11.9.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.4",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "loader.js": "4.7.0",
    "miragejs": "0.1.47",
    "npm-package-json-lint": "6.4.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "qunit": "2.19.4",
    "qunit-dom": "2.0.0",
    "sass": "1.63.6",
    "webpack": "5.88.1"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": ">= 18.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release": {
    "extends": "@adfinis/semantic-release-config"
  },
  "//": [
    "TODO(miragejs): conflicts with ember-cli-mirage in floating dependencies test"
  ],
  "resolutions": {
    "miragejs": "0.1.47"
  }
}
